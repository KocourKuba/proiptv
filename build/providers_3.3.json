    "drm": {
      "json_source": "http://epg.drm-play.com/{PROVIDER}%2Fepg%2F{EPG_ID}.json",
      "parser": {
        "epg_root": "epg_data",
        "epg_name": "name",
        "epg_desc": "descr",
        "epg_start": "time",
        "epg_end": "time_to"
      }
    },
    "itvlive": {
      "json_source": "{API}/epg/{EPG_ID}",
      "parser": {
        "epg_root": "res",
        "epg_name": "title",
        "epg_desc": "desc",
        "epg_start": "startTime",
        "epg_end": "stopTime"
      }
    },
    "ottclub": {
      "json_source": "{API}/api/channel/{ID}",
      "parser": {
        "epg_root": "epg_data",
        "epg_name": "name",
        "epg_desc": "descr",
        "epg_start": "time",
        "epg_end": "time_to"
      }
    },
    "shuratv": {
      "json_source": "http://epg.propg.net/{EPG_ID}/epg2/{YEAR}-{MONTH}-{DAY}",
      "parser": {
        "epg_root": "",
