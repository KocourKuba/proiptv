  "epg_presets": {
    "cbilling": {
      "json_source": "{API}/epg/{EPG_ID}/?date=",
      "parser": {
        "epg_root": "",
        "epg_name": "name",
        "epg_desc": "descr",
        "epg_start": "time",
        "epg_end": "time_to"
      }
