        "epg_desc": "desc",
        "epg_start": "start",
        "epg_end": "stop"
      }
    },
    "sharaclub": {
      "json_source": "http://api.playtv.pro/get/?type=epg&ch={EPG_ID}",
      "parser": {
        "epg_root": "",
        "epg_name": "name",
        "epg_desc": "descr",
        "epg_start": "time",
        "epg_end": "time_to"
      }
    },
    "sharavoz": {
      "json_source": "{API}/program?epg={ID}&date=",
      "parser": {
        "epg_root": "epg_data",
        "epg_name": "name",
        "epg_desc": "descr",
        "epg_start": "time",
        "epg_end": "time_to"
      }
    },
    "tvclub": {
      "json_source": "{API}/epg?token={TOKEN}&channels={EPG_ID}&time={TIMESTAMP}&period=24",
      "parser": {
        "epg_root": "epg|channels|[0]|epg",
        "epg_name": "text",
        "epg_desc": "description",
        "epg_start": "start",
        "epg_end": "end"
